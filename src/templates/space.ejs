<% if (!space) { %>

  <p>Intro. <a href="#/signin">Sign in</a> or <a href="#/signup">sign up</a>.</p>

<% } else if (space.relationships.length > 0) { %>

  <% _.each(space.relationships, function(r){ %>

    <p><%= r.name %></p>

  <% }) %>

<% } else { %>

  <p>No relationships yet. <a href="#/relationships/add">Add one</a>.</p>

<% } %>
